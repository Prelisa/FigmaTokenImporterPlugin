{
  "compilerOptions": {
    "target": "ES5",
    "module": "esnext",
    "lib": ["ES2020", "DOM"],
    "outDir": "./dist",
    "rootDir": "./",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  },
  "include": ["src/code.ts"],
  "exclude": ["node_modules", "dist", "ui"]
}