{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "lib": ["ES2020", "DOM"],
    "outDir": "./dist/ui",
    "rootDir": "./ui",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  },
  "include": ["ui/ui.ts"],
  "exclude": ["node_modules", "dist", "src"]
}